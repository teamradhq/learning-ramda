{
  "name": "learning-ramda",
  "license": "MIT",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "lint:ci": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --max-warnings 0",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "NODE_ENV=test jest --config jest.config.ts",
    "test:ci": "NODE_ENV=test jest --config jest.config.ts --testPathIgnorePatterns jest",
    "test:init": "ts-node -r tsconfig-paths/register ./init/init.ts",
    "test:coverage": "npm run test -- --coverage --testPathIgnorePatterns jest"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.10",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@types/ramda": "^0.30.0",
    "ramda": "^0.30.0"
  }
}
